    "\n",
    "# make_url('naver')\n",
    "# www.naver.com\n",
    "\n",
    "def make_url(site):\n",
    "    url = 'www.' + site + '.com'\n",
    "    return url\n",
    "\n",
    "print(make_url('naver'))"
